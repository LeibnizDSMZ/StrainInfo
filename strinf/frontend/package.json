{
    "name": "@straininfo/frontend",
    "type": "module",
    "private": true,
    "scripts": {
        "build": "tsc -p ../../$CONFIG_FE_TSC && vite build --config ../../$CONFIG_FE_VITE",
        "serve": "vite preview --config ../../$CONFIG_FE_VITE",
        "dev": "bunx --bun vite --config ../../$CONFIG_FE_VITE",
        "lint": "bun run lint:prune && bun run lint:eslint ../../$STRINF_FRONTEND && bun run lint:es-mdx ../../$STRINF_FRONTEND --fix",
        "lint:prune": "knip --tsConfig ../../$CONFIG_FE_TSC_APP --config ../../$CONFIG_FE_KNIP",
        "lint:eslint": "eslint --fix -c ../../$CONFIG_FE_ESLINT",
        "lint:es-mdx": "eslint --fix -c ../../$CONFIG_FE_ESLINT_MDX"
    },
    "dependencies": {
        "@extra/straininfo": "file:../../extra/straininfo",
        "@floating-ui/dom": "1.7.3",
        "@phosphor-icons/web": "2.1.2",
        "d3": "7.9.0",
        "d3-sankey": "0.12.3",
        "digidive": "file:../../extra/style",
        "driver.js": "1.3.6",
        "highlight.js": "11.11.1",
        "js-yaml": "4.1.0",
        "preact": "10.27.2",
        "preact-iso": "2.11.0",
        "rapidoc": "9.3.8",
        "rehype-highlight": "7.0.2",
        "remark-gfm": "4.0.1",
        "remark-images": "4.1.0",
        "zod": "4.1.11"
    },
    "devDependencies": {
        "@antfu/eslint-config": "6.0.0",
        "@eslint-react/eslint-plugin": "2.0.6",
        "@mdx-js/mdx": "3.1.1",
        "@mdx-js/rollup": "3.1.1",
        "@preact/preset-vite": "2.10.2",
        "@types/d3": "7.4.3",
        "@types/d3-sankey": "0.12.4",
        "@types/js-yaml": "4.0.9",
        "@typescript-eslint/parser": "8.46.1",
        "browserslist": "4.26.3",
        "eslint": "9.37.0",
        "eslint-plugin-mdx": "3.6.2",
        "eslint-plugin-react-hooks": "7.0.0",
        "eslint-plugin-react-refresh": "0.4.23",
        "knip": "5.64.2",
        "lightningcss": "1.30.2",
        "purgecss": "7.0.2",
        "rollup-plugin-license": "3.6.0",
        "rollup-plugin-visualizer": "6.0.4",
        "sharp": "0.34.3",
        "svgo": "4.0.0",
        "typescript-plugin-css-modules": "5.2.0",
        "vite": "7.1.11",
        "vite-plugin-image-optimizer": "2.0.2"
    }
}
